#include <unistd.h>

void	ft_p(char c)
{
	write(1, &c, 1);
}

void	ft_print_comb2(void)
{
	int	a;
	int	b;

	a = 0;
	while (a <= 98)
	{
		b = a + 1;
		while (b <= 99)
		{
			ft_p((a / 10) + '0');
			ft_p((a % 10) + '0');
			ft_p(' ');
			ft_p((b / 10) + '0');
			ft_p((b % 10) + '0');
			if (!(a == 98 && b == 99))
			{
				ft_p(',');
				ft_p(' ');
			}
			b++;
		}
		a++;
	}
}
